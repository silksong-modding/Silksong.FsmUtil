{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "**.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "xref": [
      "https://learn.microsoft.com/en-us/dotnet/.xrefmap.json",
      "https://normanderwan.github.io/UnityXrefMaps/6000.0/xrefmap.yml"
    ],
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern", "silksong-modding-style-docfx"],
    "globalMetadata": {
      "_appName": "FsmUtil",
      "_appTitle": "FsmUtil",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
